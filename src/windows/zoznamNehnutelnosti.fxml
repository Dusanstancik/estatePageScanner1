            <TableColumn fx:id="column7" prefWidth="300.0" text="titulka" />
        </columns>
      </TableView>

      <ComboBox fx:id="cbxServer" layoutX="21.0" layoutY="21.0" onAction="#ActionSortByServer" prefWidth="150.0" />
      <ComboBox layoutX="270.0" layoutY="21.0" prefHeight="25.0" prefWidth="101.0" />
      <ComboBox layoutX="381.0" layoutY="21.0" prefWidth="150.0" />
      <Button fx:id="btnDelete" layoutX="969.0" layoutY="21.0" mnemonicParsing="false" onAction="#ActionDelete" text="Vymaž záznam" />
